{% extends 'header.j2' %}{% block content %}
    <request verb="ListIdentifiers" metadataPrefix="oai_dc" from="{{ current_date_time }}" >
        {{ project_data.base_url }}/oai-pmh
    </request>
    <ListIdentifiers>{% for object in object_list %}
        <header>
            <identifier>{{ object.id }}</identifier>
            <datestamp>{{ object.datestamp }}</datestamp>
        </header>{% endfor %}
    </ListIdentifiers>
{% endblock %}